{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "andreaselia/laravel-api-to-postman": "^2.1",
        "arcanedev/log-viewer": "^12.0",
        "astrotomic/laravel-translatable": "^11.12",
        "diglactic/laravel-breadcrumbs": "^10.0",
        "doctrine/dbal": "^3.7",
        "guzzlehttp/guzzle": "^7.8",
        "hisorange/browser-detect": "^5.0",
        "jenssegers/date": "^2.0",
        "lab404/laravel-impersonate": "^1.7",
        "laracasts/flash": "^3.2",
        "laracasts/presenter": "^0.2.4",
        "laraeast/laravel-bootstrap-forms": "^v10.2.0",
        "laraeast/laravel-settings": "^2.0",
        "laravel-notification-channels/onesignal": "^2.5",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.1",
        "laravel/tinker": "^3.0",
        "laravel/ui": "^4.6",
        "nwidart/laravel-modules": "^11.0",
        "pusher/pusher-php-server": "^7.2",
        "santigarcor/laratrust": "^8.5",
        "spatie/laravel-backup": "^9.0",
        "spatie/laravel-medialibrary": "^11.13",
        "tightenco/parental": "^1.4",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.40",
        "laraeast/laravel-vue-i18n-generator": "^3.0",
        "laravel/sail": "^1.27",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.8",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeders"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "php-cs:issues": "vendor/bin/php-cs-fixer fix --diff --dry-run",
        "php-cs:fix": "vendor/bin/php-cs-fixer fix",
        "app:clear": "php artisan clear-compiled && php artisan cache:clear && php artisan config:clear && php artisan debugbar:clear && php artisan route:clear && php artisan view:clear",
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan vendor:publish --tag=locales:flags"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --tag=locales:flags"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan vendor:publish --tag=locales:flags"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
